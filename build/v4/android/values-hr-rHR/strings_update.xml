    <string name="update_notification_text">Verzija %1$s je spremna za preuzimanje! Dodirnite za nastavak.</string>
    <!-- Header displaying the new app version. Important! Parameter was removed from this string, so please remove any variable placeholders from translations. -->
    <string name="update_notification_channel">Obavijesti o ažuriranjima</string>
    <string name="update_info">Ako ste preuzeli aplikaciju s Play Store, F-Droid ili druge trgovine, primit ćete ažuriranja iz trgovine čim postanu dostupna. Provjerite je li Blokada deaktivirana tijekom ažuriranja. Ako imate problema prilikom ažuriranja, idite na blokada.org i preuzmite aplikaciju ručno. Nema potrebe da unaprijed deinstalirate postojeću verziju.</string>
    <!-- Displayed in the home screen when there is an update. Keep it short. -->
    <string name="update_dash_available">&lt;b&gt;Ažuriranje dostupno!&lt;/b&gt;</string>
    <!-- Displayed in the home screen whenever there is no update. Keep it short. -->
    <!-- Displayed in menu to open the About/Update screen. -->
    <!-- A button that opens system preferences for this app. -->
    <string name="update_button_appinfo">App details</string>
    <!-- A button that opens the changelog. -->
    <!-- A toast message displayed when the update process is starting. -->
    <string name="update_starting">Počinje ažuriranje, molimo pričekajte&#8230;</string>
    <string name="update_progress">Downloading: %s%%</string>
    <string name="update_complete">Download complete</string>
    <string name="update_failed">Download failed</string>
    <string name="update_instruction">When finished, you will be asked to allow Blokada to install the update.</string>
    <string name="update_instruction_failed">Tap here to try again, or long tap to download with your browser instead.</string>
