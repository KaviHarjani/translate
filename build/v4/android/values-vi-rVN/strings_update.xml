<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="update_button">Tải về bản cập nhật</string>
    <string name="update_notification_title">Bản cập nhật sẵn sàng</string>
    <!-- Parameter: new app version, for example 2.0.1 -->
