    <string name="update_button_appinfo">Chi tiết ứng dụng</string>
    <!-- A button that opens the changelog. -->
    <!-- A toast message displayed when the update process is starting. -->
    <string name="update_starting">Đang khởi chạy, vui lòng chờ&#8230;</string>
    <string name="update_progress">Đang tải xuống: %s%%</string>
    <string name="update_complete">Tải xuống hoàn tất</string>
    <string name="update_failed">Tải xuống thất bại</string>
    <string name="update_instruction">Khi hoàn tất, bạn sẽ được yêu cầu cho phép Blokada cài đặt bản cập nhật.</string>
    <string name="update_instruction_failed">Nhấn vào đây để thử lại hoặc nhấn và giữ để tải xuống bằng trình duyệt của bạn.</string>
